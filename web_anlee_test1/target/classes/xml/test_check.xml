<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="testCheckDAO">
	<select id="selectQuestion" resultType="Map" parameterType="String">
	    SELECT test_sn
		     , qstn_sn
		FROM tb_question
	</select>
	
	<insert id="insertTest">
		INSERT INTO tb_question
		(
			test_sn
		)
		VALUES
		(
			#{testSn}
		)
	</insert>
	
	<update id="updateTest">
		UPDATE tb_question
		SET test_sn = #{testSn}
		WHERE test_sn = "test1"
	</update>
	
	<delete id="deleteTest">
		DELETE FROM tb_question
		WHERE test_sn = #{testSn}
	</delete>
	
</mapper>
