<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="testCheckDAO">
	<!-- 질문 리스트 조회 -->
	<select id="selectQuestionList" resultType="Map" parameterType="String">
	    SELECT TEST_SN
		     , QSTN_SN
		     , QSTN_CNTNT
		FROM TB_QUESTION
		WHERE TEST_SN = #{testSn}
	</select>
	
	<!-- 답변 리스트 조회 -->
	<select id="selectAnswerList" resultType="Map" parameterType="String">
	    SELECT QSTN_SN
		     , F_ANS
		     , S_ANS
		FROM TB_ANSWER
		WHERE QSTN_SN = #{qstnSn}
	</select>
</mapper>
